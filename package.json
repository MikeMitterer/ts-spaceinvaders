{
    "name": "@mmit/spaceinvaders",
    "author": "Mike Mitterer",
    "version": "0.1.9",
    "description": "Simple Space-Invader-Clone",
    "private": true,
    "type": "module",
    "scripts": {
        "version:gen": "package2json.sh -g",
        "version:pkg": "jq -r '.version' < package.json",
        "version:pkg:full": "echo $(jq -r -j '.name' < package.json;echo ': ';yarn version:pkg)",
        "version:remote": "curl --silent -X GET -H \"Authorization: token ${GH_TOKEN_READ_PRIVATE_REPO}\" \"https://raw.githubusercontent.com/$(cat ./.gitorg)/$(git repo)/master/package.json\" | jq -r \".version\" ",
        "version:remote:full": "echo $(jq -r -j '.name' < package.json;echo ': ';yarn version:remote)",
        "update": "yarn update:browser",
        "update:browser": "browserslist --update-db",
        "update:templates": "updateDevTemplates.sh -u",
        "clean": "rm -rf lib dist bin",
        "clean:jest": "jest --clearCache",
        "clean:all": "yarn clean && yarn clean:jest && rm -f yarn.lock && rm -rf node_modules && yarn clean cache",
        "start": "vite --mode development --open",
        "tc": "tsc --noEmit --project tsconfig.json",
        "lint": "yarn eslint src tests",
        "lint:fix": "yarn eslint src --fix",
        "test": "yarn run test:unit",
        "test:unit": "jest --colors tests/unit",
        "build": "yarn run --silent version:gen dev > public/app.json && vite --mode development build",
        "build:prod": "yarn run --silent version:gen production > public/app.json && vite --mode production build",
        "deploy": "test $(yarn --version | awk -F. '{ print $1 }') -eq 4 && yarn run deploy:y4 || yarn run deploy:y2",
        "deploy:y2": "yarn version --new-version patch && yarn publish --non-interactive",
        "deploy:y4": "yarn version patch && yarn npm publish --access restricted"
    },
    "dependencies": {
        "@mmit/logging": "0.1.13",
        "@mmit/validate": "0.3.14",
        "vue": "^2.6.10",
        "vue-class-component": "^7.1.0",
        "vue-property-decorator": "^8.1.1",
        "vue-router": "^3.0.6",
        "vuex": "^3.1.1"
    },
    "devDependencies": {
        "@babel/core": "7.22.11",
        "@babel/plugin-transform-runtime": "^7.23.2",
        "@babel/preset-env": "7.22.10",
        "@babel/preset-typescript": "7.22.11",
        "@babel/runtime": "7.22.15",
        "@types/jest": "29.5.4",
        "@typescript-eslint/eslint-plugin": "6.7.4",
        "@typescript-eslint/parser": "6.7.4",
        "@vitejs/plugin-vue2": "2.2.0",
        "@vue/test-utils": "1",
        "@vue/vue2-jest": "29.2.4",
        "babel-jest": "29.7.0",
        "babel-plugin-transform-inline-environment-variables": "^0.4.3",
        "babel-preset-env": "^1.7.0",
        "date-fns": "2.23.0",
        "eslint": "8",
        "eslint-plugin-vue": "9.17.0",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-environment-node": "29.7.0",
        "jest-extended": "2.0.0",
        "keycloak-js": "15.0.1",
        "moment": "^2.24.0",
        "sass": "~1.32",
        "ts-jest": "^29.1.1",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "typescript-tslint-plugin": "^0.4.0",
        "vite": "4.4.12",
        "vite-plugin-ejs": "1.6.4",
        "vite-plugin-node-polyfills": "0.15.0",
        "vite-plugin-pwa": "0.16.5",
        "vue-demi": "0.14.6",
        "vue-eslint-parser": "9.3.2",
        "vue-template-compiler": "2.7",
        "vue-tsc": "1.8.15",
        "vuetify2-component-types": "2",
        "vuex-module-decorators": "^0.9.9"
    }
}
